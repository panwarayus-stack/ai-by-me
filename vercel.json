{
  "version": 2,
  "name": "custom-encoder",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": null
}
